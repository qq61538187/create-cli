    "commander": "^8.3.0",
    "download": "^8.0.0",
    "fs-extra": "^11.3.0",
    "ini": "4.1.3",
    "inquirer": "^12.6.3",
    "open": "8.4.2",
    "ora": "^8.2.0",
    "undici": "5.28.2"
